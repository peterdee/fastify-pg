{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "import/extensions": [
        "error",
        {
          "js": "always"
        }
      ],
      "max-len": [
        "error",
        {
          "code": 100
        }
      ]
    }
}
